    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "react-native-typescript-transformer": "^1.2.13"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
