{
  "name": "lifebuddy-mobile",
  "version": "1.0.0",
  "description": "LifeBuddy Mobile App - AI-powered personal growth",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:simulator": "react-native run-ios --simulator='iPhone 16 Pro'",
    "ios:device": "react-native run-ios --device",
    "ios:clean": "cd ios && xcodebuild clean -workspace LifeBuddyDev.xcworkspace -scheme LifeBuddyDev",
    "ios:build": "cd ios && xcodebuild -workspace LifeBuddyDev.xcworkspace -scheme LifeBuddyDev -configuration Debug build",
    "ios:archive": "cd ios && xcodebuild -workspace LifeBuddyDev.xcworkspace -scheme LifeBuddyDev -configuration Release archive",
    "start": "react-native start --port 8083",
    "start:metro": "react-native start --port 8083 --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace LifeBuddyDev.xcworkspace -scheme LifeBuddyDev -configuration Release",
    "pod-install": "cd ios && pod install",
    "pod-update": "cd ios && pod update",
    "pod-deintegrate": "cd ios && pod deintegrate",
    "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "clean:ios": "cd ios && xcodebuild clean && rm -rf build",
    "clean:android": "cd android && ./gradlew clean",
    "clean:all": "rm -rf node_modules && cd ios && rm -rf Pods && cd .. && pnpm install && pnpm run pod-install",
    "install:all": "pnpm install && pnpm run pod-install",
    "dev": "pnpm start",
    "dev:android": "pnpm run android",
    "dev:ios": "pnpm run ios:simulator",
    "dev:ios:device": "pnpm run ios:device",
    "setup:ios": "pnpm run pod-install && pnpm run ios:clean",
    "reset:ios": "pnpm run clean:ios && pnpm run pod-deintegrate && pnpm run pod-install",
    "doctor": "npx react-native doctor",
    "info": "npx react-native info"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.75.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "react-native-vector-icons": "^10.0.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-blur": "^3.2.2",
    "react-native-svg": "^13.14.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-modal": "^13.0.1",
    "react-native-picker-select": "^8.1.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-image-picker": "^5.6.0",
    "react-native-audio-recorder-player": "^3.5.3",
    "react-native-sound": "^0.11.2",
    "react-native-permissions": "^3.10.1",
    "react-native-camera": "^4.2.1",
    "react-native-fs": "^2.20.0",
    "react-native-share": "^9.4.1",
    "react-native-haptic-feedback": "^2.1.0",
    "react-native-device-info": "^10.11.0",
