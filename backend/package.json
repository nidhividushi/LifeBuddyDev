{
  "name": "lifebuddy-backend",
  "version": "1.0.0",
  "description": "LifeBuddy API Server - AI-powered personal growth backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node src/database/migrations/index.ts",
    "seed": "ts-node src/database/seeds/index.ts",
    "docker:build": "docker build -t lifebuddy-backend .",
    "docker:run": "docker run -p 3000:3000 lifebuddy-backend"
  },
  "keywords": [
    "api",
    "nodejs",
    "express",
    "mongodb",
    "ai",
    "goals",
    "productivity"
  ],
  "author": "LifeBuddy Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^7.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "mongoose": "^8.0.3",
    "redis": "^4.6.10",
    "elasticsearch": "^16.7.3",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1490.0",
    "openai": "^4.20.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "firebase-admin": "^11.11.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "sharp": "^0.32.6",
    "ffmpeg-static": "^5.2.0",
    "speech": "^0.2.1",
    "natural": "^6.8.0",
    "sentiment": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/sharp": "^0.32.0",
    "@types/natural": "^5.1.5",
    "@types/sentiment": "^5.0.4",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts",
      "!src/database/migrations/**",
      "!src/database/seeds/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
} 